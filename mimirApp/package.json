    "@react-native-firebase/functions": "^12.0.0",
    "formik": "^2.2.8",
    "react": "17.0.1",
    "react-native": "0.64.1",
    "react-native-paper": "^4.9.1",
    "react-router-native": "^5.2.0",
    "tslint": "^6.1.3",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "7.14.3",
    "@babel/runtime": "7.14.0",
    "@react-native-community/eslint-config": "2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.8",
